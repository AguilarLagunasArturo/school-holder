<Window x:Class="Ventanas.wCaptura"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ventanas"
        mc:Ignorable="d"
        Title="wCaptura" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="162" Margin="10,10,0,0" VerticalAlignment="Top" Width="347">
            <TextBox x:Name="textNombre" Height="23" TextWrapping="Wrap"/>
            <TextBox x:Name="textCalificacion" Height="23" TextWrapping="Wrap"/>
            <Button x:Name="buttonAceptar" Content="Aceptar" Click="buttonAceptar_Click"/>
        </StackPanel>

    </Grid>
</Window>
