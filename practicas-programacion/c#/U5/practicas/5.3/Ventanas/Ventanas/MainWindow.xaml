<Window x:Class="Ventanas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ventanas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="comboAlumnos" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="comboAlumnos_SelectionChanged"/>
        <Button x:Name="buttonRegistro" Content="Registro" HorizontalAlignment="Left" Margin="146,12,0,0" VerticalAlignment="Top" Width="75" Click="buttonRegistro_Click"/>

    </Grid>
</Window>
